{
    "name": "__MSG_extension_name__",
    "version": "0.2",
    "description": "__MSG_extension_description__",
    "browser_action":{
        "default_icon": "images.jpg",
        "popup": "options.html"
    },
    "icons":{
        "16": "images.jpg",
        "48": "images.jpg"
    },
    "background_page": "background.html",
